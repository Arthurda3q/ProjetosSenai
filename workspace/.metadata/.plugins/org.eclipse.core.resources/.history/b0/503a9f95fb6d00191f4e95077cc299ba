import java.util.Random;

import javax.swing.JOptionPane;


public class BuscaPalavra {

	public static void main(String[] args) {
		Random random = new Random();
		int[][] matz = new int[10][10];
		
		String palavra = "java".toUpperCase();
		int il = random.nextInt(10); //Indice da linha
		int ic = random.nextInt(10); //Indice da coluna
		
		char c = 0;
		for (int i = 0; i < 250; i++) {
			c = (char) i;
			JOptionPane.showMessageDialog(null, c+" "+i);
		}
		
	}

}
