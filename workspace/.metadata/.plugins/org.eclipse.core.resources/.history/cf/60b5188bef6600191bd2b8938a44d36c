import javax.swing.JOptionPane;

public class GerenciarPessoa {

	public static void main(String[] args) {

		Cliente cliente = new Cliente("Arthur Souza", 1100);
		cliente.setIdade(17);
		cliente.setCpf("049.297.831-00");
		cliente.setSexo("Masculino");
		cliente.setTelefone("98272-6392");

		JOptionPane.showMessageDialog(
				null,"Nome: "+
				cliente.getNome() + "\n" +"Renda: "+ cliente.getRenda() + "\n"
						+"Idade: "+ cliente.getIdade() + "\n" +"Cpf: "+ cliente.getCpf() + "\n"
						+"Sexo: "+ cliente.getSexo() + "\n" +"tel: "+ cliente.getTelefone());

	}

}
